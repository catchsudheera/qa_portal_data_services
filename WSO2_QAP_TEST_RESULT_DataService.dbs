      <param name="WSO2_QAP_TEST_STATUS" ordinal="2" paramType="SCALAR" sqlType="STRING" type="IN"/>
      <param name="WSO2_QAP_AUTOMATION_TEST_RESULT" ordinal="3" paramType="SCALAR" sqlType="STRING" type="IN"/>
      <param name="WSO2_QAP_TESTED_BY" ordinal="4" paramType="SCALAR" sqlType="STRING" type="IN"/>
   </query>
   <query id="update_WSO2_QAP_TEST_RESULT_query" useConfig="default">
